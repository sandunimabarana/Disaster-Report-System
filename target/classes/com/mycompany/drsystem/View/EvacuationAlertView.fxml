<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.drsystem.Controller.EvacuationAlertController">
   <children>
      <TableView fx:id="alertTable" layoutX="14.0" layoutY="14.0" prefHeight="376.0" prefWidth="572.0">
        <columns>
          <TableColumn fx:id="alertIdColumn" prefWidth="75.0" text="Alert ID" />
          <TableColumn fx:id="disasterIdColumn" prefWidth="75.0" text="Disaster ID" />
          <TableColumn fx:id="alertMessageColumn" prefWidth="200.0" text="Alert Message" />
          <TableColumn fx:id="sentByColumn" prefWidth="100.0" text="Sent By" />
          <TableColumn fx:id="sentAtColumn" prefWidth="125.0" text="Sent At" />
          <TableColumn fx:id="statusColumn" prefWidth="100.0" text="Status" />
          <TableColumn fx:id="removeEvacuationColumn" prefWidth="150.0" text="Remove Evacuation" />
        </columns>
      </TableView>
      <Button fx:id="backButton" layoutX="300.0" layoutY="150.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
   </children>
</AnchorPane>
