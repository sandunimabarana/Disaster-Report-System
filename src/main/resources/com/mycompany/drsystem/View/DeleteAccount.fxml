      <Button fx:id="deleteButton" layoutX="200.0" layoutY="150.0" mnemonicParsing="false" onAction="#deleteAccount" text="Delete" />
      <Button fx:id="backButton" layoutX="300.0" layoutY="150.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
   </children>
