<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.drsystem.Controller.CreateAccountController">
    <children>
      <Label layoutX="100.0" layoutY="30.0" text="Create Account" />
      <Label layoutX="100.0" layoutY="80.0" text="Department:" />
      <ComboBox fx:id="departmentComboBox" layoutX="180.0" layoutY="75.0" prefWidth="200.0" />
      <Label layoutX="100.0" layoutY="130.0" text="Username:" />
      <TextField fx:id="usernameTextField" layoutX="180.0" layoutY="125.0" prefWidth="200.0" />
      <Label layoutX="100.0" layoutY="180.0" text="Password:" />
      <PasswordField fx:id="passwordField" layoutX="180.0" layoutY="175.0" prefWidth="200.0" />
      <Label layoutX="100.0" layoutY="230.0" text="Email:" />
      <TextField fx:id="emailTextField" layoutX="180.0" layoutY="225.0" prefWidth="200.0" />
      <Button fx:id="saveButton" layoutX="180.0" layoutY="280.0" mnemonicParsing="false" onAction="#saveAccount" text="Save" />
      <Button fx:id="backButton" layoutX="210.0" layoutY="150.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
   </children>  
</AnchorPane>
